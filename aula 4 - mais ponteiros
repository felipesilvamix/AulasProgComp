// mais ponteiros
#include <iostream>
using namespace std;

int main ()
{
	int firstvalue = 5, secondvalue = 15;
	int * p1, * p2;
	
	p1 = &firstvalue; // p1 = address of firstvalue
	p2 = &secondvalue; 
	*p1 = 10;
	*p2 = *p1;
	p1 = p2;
	*p1 = 20;
		cout << "O primeiro valor e " << firstvalue <<'\n';
	cout<< "O segundo valor e " << secondvalue << '\n';
	return 0;
}
